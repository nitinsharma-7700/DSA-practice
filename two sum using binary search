#include <iostream>
#include <vector>
using namespace std;

int main() {
int a[]={1,2,4,6,7};
int target=11;
for(int i=0;i<4;i++){
    int x=target-a[i];
    int start=i+1,end=4;
    while(start<=end){
        int mid=start+(end-start)/2;
        if(a[mid]==x){
            cout<<a[i]<<","<<a[mid];
            break;
            
        }
        else if(a[mid]>x){
            end=mid-1;
        }
        else start=mid+1;
    }
}


}

