#include <iostream>
#include <vector>
#include <algorithm>
#include <climits>

using namespace std;
vector<int> find(vector<int> a, int target){
      int ans;
      int n=a.size();
    for(int i=0;i<n-3;i++){
        for(int j=i+1;j<n-2;j++){
            ans=target-a[i]-a[j];
            int start=j+1,end=n-1;
            while(start<end){
                if(a[start]+a[end]==ans){
                    return {i,j,start,end};
                }
                else if(a[start]+a[end]>ans){
                    end--;
                }
                else start++;
            }
        }
    }
    return {};
}


int main(){
    int n;
    cin>>n;
    vector<int>a(n);
    for(int i=0;i<n;i++){
        cin>>a[i];
    }
    vector<int> result = find(a,33);
    for(int i=0; i<result.size(); i++){
        cout<<result[i]<<" ";
    }
   
    

}
