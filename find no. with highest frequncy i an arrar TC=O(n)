class Solution {
public:
    int majorityElement(vector<int>& nums) {
        int n=nums.size();
        vector<int>count(n);
        for(int i=0;i<n;i++){
            count[nums[i]-1]++;
        }
        int maxi=INT_MIN;
        int idx;
        for(int i=0;i<n;i++){
            
            if(count[i]>maxi){
                maxi=count[i];
              idx=i+1;}
        }
        return idx;
        
    }
};
