#include <iostream>
#include <vector>
#include <algorithm>

using namespace std;
int equal_pair(vector<int> arr){
    int s=arr.size();
    
    int total_sum=0;
    for(int i=0;i<s;i++){
        total_sum+=arr[i];
        
        
    }
    int prefix=0;
    
    for(int i=0;i<s-1;i++){
        prefix+=arr[i];
        int sum=total_sum-prefix;
        if(prefix==sum){
            return sum;
        }
    }
    return 0;
    
}

int main(){
    int n;
    cin>>n;
    
   vector<int> arr(n);
   for(int i=0;i<n;i++){
       cin>>arr[i];
   }
   cout<<equal_pair(arr);
   
    

}
