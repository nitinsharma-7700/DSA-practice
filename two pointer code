#include <iostream>
#include <vector>
using namespace std;

int main() {
// 	two pointers
int a[10]={1,0,1,0,1,0,0,1,1,0};
int start=0,end=9;
while(start<end){
    if(a[start]==0){
        start++;
        
    }
    else{
        if(a[end]==0){
            swap(a[start],a[end]);
            start++,end--;
        }
        else{
            end--;
        }
    }
}
for(int i=0;i<10;i++){
    cout<<a[i]<<" ";
}

}

