#include <iostream>
#include <vector>
#include <algorithm>
#include <climits>

using namespace std;


int main(){
    vector<int>v={3,4,-5,8,-12,7,6,-2};
    int maxi=INT_MIN;
    int prefix=0;
    for(int i=0;i<8;i++){
        prefix+=v[i];
        maxi=max(prefix,maxi);
        if(prefix<0){
            prefix=0;
        }
    }
    cout<<maxi;
    

}
